package com.sophossolutions.actions;

import org.openqa.selenium.By;
import org.openqa.selenium.WebDriver;

public class Action {


	//Buscar productos
	public static void buscarProducto(WebDriver driver, 
			By byTextoBuscar, By byBtnBuscar, 
			String strProducto) {
		
		driver.findElement(byTextoBuscar).sendKeys(strProducto);
		driver.findElement(byBtnBuscar).click();
	}
	
	//Seleccionar mejor calificado
	public static void seleccionarMejorCalificado(WebDriver driver, By byResultados) {
		// TODO Auto-generated method stub
		
	}

}
